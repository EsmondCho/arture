	jindo.$Fn(function() {
		initUserPoint('17421');
		
		pointHelpWideLayerToggle = function() {
			setTimeout( function() {
				if (document.activeElement != null) {
					var focusedEl = jindo.$Element(document.activeElement);
					if (focusedEl != null) {
						if ( !focusedEl.hasClass("_pointHelpWide") ) {
							 jindo.$Element("pointHelpWide").hide();
						}
					}
				}
			}, 100);
		};
	
	//개봉전 평점 개선 관련. 개봉전 영화인 경우 평점을 보여주지 않는다.
