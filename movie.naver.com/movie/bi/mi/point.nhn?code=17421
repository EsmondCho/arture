<script type="text/javascript">
jindo.$Fn(function () {
    var welTopAnchor = jindo.$Element('floatingTopAnchor');
    var welContent = jindo.$Element('content');
    var nTopMargin = 342 + welTopAnchor.height();
    var nMaxY;
    var nMinY = 61;
    var oFloatingLayer = new jindo.FloatingLayer(welTopAnchor).attach({
        beforeMove: function (oEvent) {
            // 상단 limit
            nMaxY = welContent.height() - nTopMargin;
            if (oEvent.nY > nMaxY) {
                oEvent.nY = nMaxY;
            }

             // 하단 limit
            if (oEvent.nY < nMinY) {
                oEvent.nY = nMinY;
            }
        }
    });
}).attach(window, 'load');
</script>
</div>
<script type="text/javascript">

var isReviewListLoad = false;
var isPointAfterListLoad = false;
var isPointBeforeListLoad = false;

function setParamForPointAfterList(pointAfterActualPointYn, pointAfterOrder, pointAfterPage) {
	jindo.$("pointAfterActualPointYn").value = pointAfterActualPointYn;
	jindo.$("pointAfterOrder").value = pointAfterOrder;
	jindo.$("pointAfterPage").value = pointAfterPage;
}

function setParamForPointBeforeList(pointBeforeInterestYn, pointBeforePage) {
	jindo.$("pointBeforeInterestYn").value = pointBeforeInterestYn;
	jindo.$("pointBeforePage").value = pointBeforePage;
}

function setParamForReviewList(reviewOrder, reviewPage) {
	jindo.$Element("reviewOrder").value = reviewOrder;
	jindo.$Element("reviewPage").value = reviewPage;
}

function getParamForExcludeReview() {
	var paramTotalSubPage = "&pointAfterActualPointYn=" + jindo.$("pointAfterActualPointYn").value
		+ "&pointAfterOrder=" + jindo.$("pointAfterOrder").value
		+ "&pointAfterPage=" + jindo.$("pointAfterPage").value
	 	+ "&pointBeforeInterestYn=" + jindo.$("pointBeforeInterestYn").value
		+ "&pointBeforePage=" + jindo.$("pointBeforePage").value;
	
	return paramTotalSubPage;
}

var writeInterest = '';

function setWriteInterest( mode ){
	writeInterest = mode;
}
function getWriteInterest( mode ){
	return writeInterest;
}

function resizePointAfterListIframe(height) {
	jindo.$("pointAfterListIframe").height = height + "px";
}

function resizePointBeforeListIframe(height) {
	jindo.$("pointBeforeListIframe").height = height + "px";
}

function resizeReviewListIframe(height) {
	jindo.$("reviewListIframe").height = height + "px";
}

function changePoint(type) {
	var netizenPointSum = "";
	
	if (type == 'before') {
		jindo.$Element("beforePointTab").className("on");
		jindo.$Element("afterPointTab").className("");

		jindo.$Element("beforePointArea").show();
		jindo.$Element("afterPointArea").hide();
		jindo.$Element("preference_area").show();
		
		if (jindo.$Element("graph_area")) {
			jindo.$Element("graph_area").hide();
			jindo.$Element("graph_comment").hide();
		}
		
		jindo.$("pointAfterListIframe").src = "/movie/bi/mi/pointWriteFormList.nhn?code=17421&type=before";
	} else if (type == 'after') {
		jindo.$Element("beforePointTab").className("");
		jindo.$Element("afterPointTab").className("on");
		if(false){
			jindo.$Element("afterPointArea").show();
		}
		jindo.$Element("beforePointArea").hide();
		jindo.$Element("preference_area").hide();
		
		if (jindo.$Element("graph_area")) {
			jindo.$Element("graph_area").show();
			jindo.$Element("graph_comment").show();
		}
		
		jindo.$("pointAfterListIframe").src = "/movie/bi/mi/pointWriteFormList.nhn?code=17421&type=after";

	}
}

var maxNetizenAgePoint = null;
var aNetizenAgePointData = null;
var oNetizenAgePointChart = null;
var aNetizenGenderPointData = null;
var oNetizenGenderPointChart = null;

var maxActualAgePoint = null;
var aActualAgePointData = null;
var oActualAgePointChart = null;
var aActualGenderPointData = null;
var oActualGenderPointChart = null;

var markInterval = null;
var intervalCount = 0;

jindo.$Fn(function() {
	
	pointHelpLayerToggle = function() {
		setTimeout( function() {
			if (document.activeElement != null) {
				var focusedEl = jindo.$Element(document.activeElement);
				if (focusedEl != null) {
					if ( !focusedEl.hasClass("_pointHelp") ) {
						 jindo.$Element("pointHelpLayer").hide();
					}
				}
			}
		}, 100);
	};

	new jindo.LayerManager("beforePointHelpLayer", {
		sCheckEvent : "click",
		nHideDelay : 0
	}).link(jindo.$("beforePointHelpLayer"), jindo.$("beforePointHelpButton"));

	jindo.$Element("beforePointHelpButton").attach("click", function(e){
		e.stop(jindo.$Event.CANCEL_DEFAULT);
		jindo.$Element("beforePointHelpLayer").toggle();
	});

	jindo.$Element("beforePointHelpCloseButton").attach("click", function(e){
		e.stop(jindo.$Event.CANCEL_DEFAULT);
		jindo.$Element("beforePointHelpLayer").toggle();
	});
	
	new jindo.LayerManager("afterPointHelpLayer", {
		sCheckEvent : "click",
		nHideDelay : 0
	}).link(jindo.$("afterPointHelpLayer"), jindo.$("afterPointHelpButton"));

	jindo.$Element("afterPointHelpButton").attach("click", function(e){
		e.stop(jindo.$Event.CANCEL_DEFAULT);
		jindo.$Element("afterPointHelpLayer").toggle();
	});

	jindo.$Element("afterPointHelpCloseButton").attach("click", function(e){
		e.stop(jindo.$Event.CANCEL_DEFAULT);
		jindo.$Element("afterPointHelpLayer").toggle();
	});

	// 평점 탭 노출 관련 로직.
	if(true) {
		
		function createNetizenGenderPointGraph() {
			if (oNetizenGenderPointChart == null || oNetizenGenderPointChart == "undefinded") {
				aNetizenGenderPointData = [
       				{ "iPer": 72.13933454023348, "sPer": 72 },
       				{ "iPer": 27.860665459766523, "sPer": 28 }
       			];
				
				oNetizenGenderPointChart = new GenderPointGraphForRaphael(jindo.$Element("netizenGenderPointGraph"), aNetizenGenderPointData);
			} else {
				oNetizenGenderPointChart.update();
			}
		}

		
		function createNetizenAgePointGraph() {
			if (oNetizenAgePointChart == null || oNetizenAgePointChart == "undefinded") {
				aNetizenAgePointData = [
       				{ "iPer": 37.32670189617656, "sPer": 37 },
       				{ "iPer": 37.60024867889338, "sPer": 38 },
       				{ "iPer": 11.296238731737644, "sPer": 11 },
       				{ "iPer": 13.776810693192415, "sPer": 14 }
       			];

       			oNetizenAgePointChart = new AgePointGraphForRaphael(jindo.$Element("netizenAgePointGraph"), aNetizenAgePointData);
			} else {
				oNetizenAgePointChart.update();
			}
		}
		
		// 최상단 탭(네티즌 평점) 클릭시 발생하는 스크립트.
		if(jindo.$Element("netizen_point_tab")){
			jindo.$Element("netizen_point_tab").attach("click", function(e){
				e.stop(jindo.$Event.CANCEL_DEFAULT);
				hideActualPointTab();
			});
			jindo.$Element("netizen_point_tab_inner").attach("click", function(e){
				e.stop(jindo.$Event.CANCEL_DEFAULT);
				hideActualPointTab();
			});
		}
		function hideActualPointTab(){
			if(!jindo.$Element("netizen_point_tab_h4").hasClass("on")) {
				jindo.$Element("netizen_point_tab_h4").addClass("on");
			}
			if(jindo.$Element("actual_point_tab_h4")) {jindo.$Element("actual_point_tab_h4").removeClass("on");}
			jindo.$Element("netizen_point_tab_div").show();
			if(jindo.$Element("actual_point_tab_div")){jindo.$Element("actual_point_tab_div").hide();}
			
			// 평점 그래프 업데이트
			createNetizenGenderPointGraph();
			createNetizenAgePointGraph();
		}
		
		//네티즌 평점 - 점수 분포 클릭시 발생하는 스크립트.
		if (jindo.$Element("netizen_point")) {
			jindo.$Element("netizen_point").attach("click", function(e){
				e.stop(jindo.$Event.CANCEL_DEFAULT);
				if(!jindo.$Element("netizen_point_h5").hasClass("on")) {
					jindo.$Element("netizen_point_h5").addClass("on");
				}
				jindo.$Element("netizen_group_h5").removeClass("on");
				jindo.$Element("netizen_point_graph").show();
				jindo.$Element("netizen_group_graph").hide();
			});
		}
		
		//네티즌 평점 - 남녀별/연령별 클릭시 발생하는 스크립트.
		if (jindo.$Element("netizen_group")) {
			jindo.$Element("netizen_group").attach("click", function(e){
				e.stop(jindo.$Event.CANCEL_DEFAULT);
				if(!jindo.$Element("netizen_group_h5").hasClass("on")) {
					jindo.$Element("netizen_group_h5").addClass("on");
				}
				jindo.$Element("netizen_point_h5").removeClass("on");
				jindo.$Element("netizen_group_graph").show();
				jindo.$Element("netizen_point_graph").hide();
				
				
				createNetizenGenderPointGraph();
				
				
				createNetizenAgePointGraph();
				
				
				if (maxNetizenAgePoint == null) {
					maxNetizenAgePoint = Math.max(9.48, 9.49, 9.34, 8.97);
					
					if (maxNetizenAgePoint < 10) {
						maxNetizenAgePoint = maxNetizenAgePoint.toFixed(2);
					} else {
						maxNetizenAgePoint = maxNetizenAgePoint.toFixed(1);
					}
					
					jindo.$A(jindo.$ElementList('._netizenAgePoint').$value()).forEach(function(value, index, array) {
						if ( value.hasClass(maxNetizenAgePoint.toString()) ) {
							value.addClass("on");
						}
					});
				}
			});
		}
		//도움말 레이어
		if(jindo.$Element("netizen_point_help_div")) {
			new jindo.LayerManager("netizen_point_help_div", {
				sCheckEvent : "click",
				nHideDelay : 0
			}).link(jindo.$("netizen_point_help_div"), jindo.$("netizen_point_help"));
			
			jindo.$Element("netizen_point_help").attach("click", function(e){
				e.stop(jindo.$Event.CANCEL_DEFAULT);
				jindo.$Element("netizen_point_help_div").toggle();
			});
		
			jindo.$Element("netizen_point_help_close").attach("click", function(e){
				e.stop(jindo.$Event.CANCEL_DEFAULT);
				jindo.$Element("netizen_point_help_div").toggle();
			});
		}
		if(jindo.$Element("actual_point_help")){
			new jindo.LayerManager("actual_point_help_div", {
				sCheckEvent : "click",
				nHideDelay : 0
			}).link(jindo.$("actual_point_help_div"), jindo.$("actual_point_help"));
			
			jindo.$Element("actual_point_help").attach("click", function(e){
				e.stop(jindo.$Event.CANCEL_DEFAULT);
				jindo.$Element("actual_point_help_div").toggle();
			});
		
			jindo.$Element("actual_point_help_close").attach("click", function(e){
				e.stop(jindo.$Event.CANCEL_DEFAULT);
				jindo.$Element("actual_point_help_div").toggle();
			});
		}
	}
	
	if(true) {
		
		function createActualGenderPointGraph() {
			if (oActualGenderPointChart == null || oActualGenderPointChart == "undefinded") {
				aActualGenderPointData = [
					{ "iPer": 46.666666666666664, "sPer": 47 },
					{ "iPer": 53.333333333333336, "sPer": 53 }
				];
			        		
				oActualGenderPointChart = new GenderPointGraphForRaphael(jindo.$Element("actualGenderPointGraph"), aActualGenderPointData);
			} else {
				oActualGenderPointChart.update();
			}
		}
	
		
		function createActualAgePointGraph() {
			if (oActualAgePointChart == null || oActualAgePointChart == "undefinded") {
				aActualAgePointData = [
					{ "iPer": 6.25, "sPer": 6 },
					{ "iPer": 75.0, "sPer": 75 },
					{ "iPer": 12.5, "sPer": 12 },
					{ "iPer": 6.25, "sPer": 6 }
				];
		
				oActualAgePointChart = new AgePointGraphForRaphael(jindo.$Element("actualAgePointGraph"), aActualAgePointData);
			} else {
				oActualAgePointChart.update();
			}
		}
		
		//최상단 탭(관람객 평점) 클릭시 발생하는 스크립트.
		if(jindo.$Element("actual_point_tab")) {
			jindo.$Element("actual_point_tab").attach("click", function(e){
				e.stop(jindo.$Event.CANCEL_DEFAULT);
				 hideNetizenPointTab();
			});
			jindo.$Element("actual_point_tab_inner").attach("click", function(e){
				e.stop(jindo.$Event.CANCEL_DEFAULT);
				 hideNetizenPointTab();
			});
		}
		function hideNetizenPointTab(){
			if(!jindo.$Element("actual_point_tab_h4").hasClass("on")) {
				jindo.$Element("actual_point_tab_h4").addClass("on");
			}
			jindo.$Element("netizen_point_tab_h4").removeClass("on");
			jindo.$Element("actual_point_tab_div").show();
			jindo.$Element("netizen_point_tab_div").hide();
			
			// 평점 그래프 업데이트
			createActualGenderPointGraph();
			createActualAgePointGraph();
		}
		
		//관람객 평점 - 점수 분포 클릭시 발생하는 스크립트.
		if(jindo.$Element("actual_point")) {
			jindo.$Element("actual_point").attach("click", function(e){
				e.stop(jindo.$Event.CANCEL_DEFAULT);
				if(!jindo.$Element("actual_point_h5").hasClass("on")) {
					jindo.$Element("actual_point_h5").addClass("on");
				}
				jindo.$Element("actual_group_h5").removeClass("on");
				jindo.$Element("actual_point_graph").show();
				jindo.$Element("actual_group_graph").hide();
			});
		}
		
		//관람객 평점 - 남녀별/연령별 클릭시 발생하는 스크립트.
		if(jindo.$Element("actual_group")){
			jindo.$Element("actual_group").attach("click", function(e){
				e.stop(jindo.$Event.CANCEL_DEFAULT);
				if(!jindo.$Element("actual_group_h5").hasClass("on")) {
					jindo.$Element("actual_group_h5").addClass("on");
				}
				jindo.$Element("actual_point_h5").removeClass("on");
				jindo.$Element("actual_group_graph").show();
				jindo.$Element("actual_point_graph").hide();
				
				
				createActualGenderPointGraph();
				
				
				createActualAgePointGraph();
				
				
				if (maxActualAgePoint == null) {
					maxActualAgePoint = Math.max(10.0, 9.83, 10.0, 10.0);
					
					if (maxActualAgePoint < 10) {
						maxActualAgePoint = maxActualAgePoint.toFixed(2);
					} else {
						maxActualAgePoint = maxActualAgePoint.toFixed(1);
					}
					
					jindo.$A(jindo.$ElementList('._actualAgePoint').$value()).forEach(function(value, index, array) {
						if ( value.hasClass(maxActualAgePoint.toString()) ) {
							value.addClass("on");
						}
					});
				}
			});
		}
	}

	
	var waelPointHelp = jindo.$ElementList('._pointHelp');

	jindo.$A(waelPointHelp.$value()).forEach(function(value, index, array) {
		jindo.$Fn(pointHelpLayerToggle, this).attach(value, "blur");
	});
    
	markInterval = setInterval(function () {
		
		intervalCount++;
		if (intervalCount > 30) {
			clearInterval(markInterval);
			markInterval = null;
		}
	
		if (isReviewListLoad && isPointAfterListLoad && isPointBeforeListLoad) {
			var url = window.location.toString();
			if (url.indexOf("#") > 0) {
				var markName = url.substring(url.indexOf("#") + 1);
				
				if (jindo.$(markName)) {
					jindo.$(markName).scrollIntoView(true);
				}
			}
			clearInterval(markInterval);
			markInterval = null;
		}
		
	}, 100);
	
}).attach(document, 'domready');

if(jindo.$Element("btn_interest_point") != null && jindo.$Element("btn_notInterest_point") != null) {
	
	//기대지수 도움말 레이어 토글.
	new jindo.LayerManager("preference_help_point", {
		sCheckEvent : "click",
		nHideDelay : 0
	}).link(jindo.$("preference_help_point"), jindo.$("preference_help_show_point"));
	
	jindo.$Element("preference_help_show_point").attach("click", function(e){
		e.stop(jindo.$Event.CANCEL_DEFAULT);
		jindo.$Element("preference_help_point").toggle();
	});
	
	jindo.$Element("preference_close_point").attach("click", function(e){
		e.stop(jindo.$Event.CANCEL_DEFAULT);
		jindo.$Element("preference_help_point").hide();
	});
	
	//
	preferenceHelpLayerToggle = function() {
		setTimeout( function() {
			if (document.activeElement != null) {
				var focusedEl = jindo.$Element(document.activeElement);
				if (focusedEl != null) {
					if ( !focusedEl.hasClass("_preferenceHelp") ) {
						 jindo.$Element("preference_help_point").hide();
					}
				}
			}
		}, 100);
	};
	
	var waelPreferenceHelp = jindo.$ElementList('._preferenceHelp');

	jindo.$A(waelPreferenceHelp.$value()).forEach(function(value, index, array) {
		jindo.$Fn(preferenceHelpLayerToggle, this).attach(value, "blur");
	});
	
	//
	
	if(false == true) {
		jindo.$Element("btn_interest_point").attach("click", function(e){
			e.stop(jindo.$Event.CANCEL_DEFAULT);
			queryAndUpdatePreference('17421','interest');
		});
		
		jindo.$Element("btn_notInterest_point").attach("click", function(e){
			e.stop(jindo.$Event.CANCEL_DEFAULT);
			queryAndUpdatePreference('17421','notInterest');
		});
	} else {
		jindo.$Element("btn_interest_point").attach("click", function(e){
			e.stop(jindo.$Event.CANCEL_DEFAULT);
			alert("국내 개봉 후에는 기대지수 등록이 불가능합니다.");
		});
		
		jindo.$Element("btn_notInterest_point").attach("click", function(e){
			e.stop(jindo.$Event.CANCEL_DEFAULT);
			alert("국내 개봉 후에는 기대지수 등록이 불가능합니다.");
		});
	}
}

</script>
